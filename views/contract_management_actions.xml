<?xml version="1.0" encoding="utf-8"?>
<odoo>
#  Contract Management Action
    <record id="action_contract_management" model="ir.actions.act_window">
        <field name="name">Contract Management</field>
        <field name="res_model">contract.management</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_sale_terms_conditions" model="ir.actions.act_window">
        <field name="name">Terms and Conditions</field>
        <field name="res_model">sale.terms.conditions</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_sale_cover_letter" model="ir.actions.act_window">
        <field name="name">Cover Letters</field>
        <field name="res_model">sale.cover.letter</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_contract_clause" model="ir.actions.act_window">
        <field name="name">Contract Clauses</field>
        <field name="res_model">contract.clause</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_contract_upload_wizard" model="ir.actions.act_window">
        <field name="name">Upload Contract</field>
        <field name="res_model">contract.upload.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>